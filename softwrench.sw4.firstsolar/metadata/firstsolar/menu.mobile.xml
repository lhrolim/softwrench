  <application id="TWO" title="Today" ref="todayworkorder" schema="list" icon="fa-table" parameters="offlinemenuwc=fsWorkorderOfflineService.getTodayWosWhereClause;offlinegridtitle=Today"/>
  <application id="PWO" title="Past" ref="pastworkorder" schema="list" icon="fa-table" parameters="offlinemenuwc=fsWorkorderOfflineService.getPastWosWhereClause;offlinegridtitle=Past"/>
  <application id="FWO" title="Future" ref="workorder" schema="list" icon="fa-table" parameters="offlinemenuwc=fsWorkorderOfflineService.getFutureWosWhereClause;offlinegridtitle=Future"/>
  <application id="WO" title="All" ref="workorder" schema="list" icon="fa-table" parameters="offlinemenuwc=fsWorkorderOfflineService.getBaseWhereClause;offlinegridtitle=All"/>

  </container>

