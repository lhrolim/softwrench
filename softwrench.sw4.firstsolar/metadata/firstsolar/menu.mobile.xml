  <container id="groupassigns" icon="fa-users fa-fw" title="Group Assignments">

    <application id="SWO" title="Scheduled" ref="schedworkorder" schema="list" icon="fa-table" parameters="offlinegridtitle=Scheduled"/>
    <application id="PNSWO" title="Planned Not Scheduled" ref="pnschedworkorder" schema="list" icon="fa-table" parameters="offlinegridtitle=Planned not Scheduled"/>
    <application id="NPNSWO" title="Not Planned Not Scheduled" ref="npnschedworkorder" schema="list" icon="fa-table" parameters="offlinegridtitle=Not Planned not Scheduled"/>
    <application id="GWO" title="Assigned to Others" ref="otherworkorder" schema="list" icon="fa-table" parameters="offlinegridtitle=Assigned to Others"/>
    <application id="UWO" title="Unassigned" ref="otherworkorderunassigned" schema="list" icon="fa-table" parameters="offlinegridtitle=Unassigned"/>
  </container>

  <application id="CWO" title="Created" ref="workorder" schema="listgeneral" icon="fa-plus" parameters="offlinemenuwc=fsWorkorderOfflineService.getCreatedWosWhereClause;offlinegridtitle=Created Work Orders"/>
  <application id="PRWO" title="Problems" ref="workorder" schema="listgeneral" icon="fa-warning" parameters="offlinemenuwc=fsWorkorderOfflineService.getProblematicWosWhereClause;offlinegridtitle=Work Orders With Problems"/>


  <container id="timesheets" title="Time Sheets" icon="fa-clock-o" >
    <application id="WTS" title="Week Time Sheet" ref="tslabor" schema="list" icon="fa-clock-o" parameters="offlinemenuwc=fsLaborOfflineService.getCurrentWeekLaborsWhereClause;offlinegridtitle=Weekly Time Sheet;offlinetititlebuilder=@firstSolarOfflineMenuService.BuildWeekTitle"/>
    <application id="MTS" title="Month Time Sheet" ref="tslabor" schema="list" icon="fa-clock-o" parameters="offlinemenuwc=fsLaborOfflineService.getCurrentMonthLaborsWhereClause;offlinegridtitle=Monthly Time Sheet;offlinetititlebuilder=@firstSolarOfflineMenuService.BuildMonthTitle"/>
  </container>



