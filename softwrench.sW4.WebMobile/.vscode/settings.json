        "jobs": [
            {
                "name": "My Script using specific date",
                "description": "Runs a script at a specific time.",
                "time": "*/5 * * * * *",
                "action": {
                    "type": "script",
                    "script": "./.vscode/vscodecron-script.js"
                }
            }
        ]
    }

