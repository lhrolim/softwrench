            <renderer type="default" params="width=15%"/>
          </field>
          <field attribute="inventory_.siteid" label="Site">
            <renderer type="default" params="width=15%"/>
          </field>
          <field attribute="location" label="Storeroom">
            <renderer type="default" params="width=16%"/>
          </field>
