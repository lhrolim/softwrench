  <commandgroup position="gridtop">
    <resourcecommand id="filter" path="/Content/Templates/commands/grid/gridfilter.html"/>
    <command id="print" icon="fa-print" service="printService" method="printList" parameters="paginationData,schema" label="Print" tooltip="Print" showexpression=""/>
    <command id="export" icon="fa-file-text-o" service="excelService" method="exporttoexcel" parameters="schema,searchData,searchSort,searchOperator,paginationData" 
             label="Export" 
             tooltip="Export to Excel" showexpression=""/>
  </commandgroup>

  <commandgroup position="gridbottom">
   
  </commandgroup>

  <!--TODO: finish bars-->
  <commandgroup position="detailform">
    <command id="save" icon="fa-save" service="$scope" method="save" label="Submit" tooltip="Submit" showexpression="$scope:shouldshowsave"/>
    <command id="print" icon="fa-print" service="printService" method="printDetail" parameters="schema,datamap" label="Print" tooltip="Print" showexpression="$scope:shouldshowprint"/>
    <command id="cancel" icon="fa-times" service="$scope" method="cancel" label="Cancel" tooltip="Cancel"/>
  </commandgroup>

  <commandgroup position="compositionbottom">
    <command id="additem" icon="$scope:getAddIcon" service="$scope" method="newDetailFn" label="$scope:getAddLabel" showexpression="$scope:shouldShowAdd" />
    <command id="save" icon="fa-save" service="$scope" method="save" label="Submit" tooltip="Submit" />
    <command id="print" icon="fa-print" service="printService" method="printDetail" parameters="parentschema,parentdatamap" label="Print" tooltip="Print" showexpression="$scope:shouldshowprint"/>
    <command id="cancel" icon="fa-times" service="$scope" method="cancel" label="Cancel" tooltip="Cancel"/>
  </commandgroup>


