<?xml version="1.0" encoding="utf-8" ?>
<metadata xmlns="http://www.example.org/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.example.org/metadata ../../Schema/Metadata.xsd ">

  <entities>
    <entity name="WOBatch"  whereclause="Application = 'WORKORDER' " idAttribute="id">
      <connectorParameters>
        <connectorParameter key="dbtable" value="Batch"/>
      </connectorParameters>
    </entity>
    
  </entities>
  
  
  <applications>


    <application entity="WOBatch" name="Batch" title="Workorder Batch">
      <schemas>
        <list title="Workorder Batches">
          <field attribute="Id" label="Batch Id"/>
          <field attribute="status" label="Status"/>
          <field attribute="UpdateDate" label="Updated"/>
          <field attribute="ItemIds" label="Workorders"/>
        </list>
      </schemas>
    </application>
     
  </applications>
</metadata>